{
  "name": "seenit",
  "version": "0.0.2",
  "scripts": {
    "start": "npm run build && node -r dotenv/config ./dist/src",
    "build": "npm run clean && tsc",
    "watch": "nodemon -e ts --exec ts-node --project ./tsconfig.json -r dotenv/config ./src",
    "clean": "rimraf dist",
    "seed": "ts-node ./seeds",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "tsc": "tsc",
    "lint": "eslint ."
  },
  "dependencies": {
    "apollo-resolvers": "^1.4.2",
    "apollo-server": "^2.12.0",
    "apollo-server-express": "^2.12.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "compression": "^1.7.3",
    "cross-spawn": "^7.0.2",
    "db-errors": "^0.2.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.17.1",
    "express-limiter": "^1.6.1",
    "graphql": "^14.6.0",
    "jest": "^25.3.0",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.20.14",
    "line-by-line": "^0.1.6",
    "morgan": "^1.10.0",
    "objection": "^2.1.2",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.0.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cross-spawn": "^6.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "1.5.0",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/passport": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.3",
    "seedrandom": "^3.0.5",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
