{
  "name": "screen",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build && node -r dotenv/config ./dist/src",
    "build": "npm run clean && tsc",
    "watch": "nodemon -e ts --exec ts-node --project ./tsconfig.json -r dotenv/config ./src",
    "clean": "rimraf dist",
    "seed": "ts-node ./seeds",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback"
  },
  "dependencies": {
    "apollo-resolvers": "^1.4.1",
    "apollo-server": "^2.5.0",
    "apollo-server-express": "^2.5.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "compression": "^1.7.3",
    "cross-spawn": "^6.0.5",
    "db-errors": "^0.2.2",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-limiter": "^1.6.1",
    "graphql": "^14.3.0",
    "jest": "^24.9.0",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.19.5",
    "line-by-line": "^0.1.6",
    "morgan": "^1.9.0",
    "objection": "^1.6.9",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "0.0.36",
    "@types/cross-spawn": "^6.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.2.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.2",
    "@types/passport": "^1.0.0",
    "nodemon": "^1.18.3",
    "seedrandom": "^3.0.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.4.5"
  }
}
